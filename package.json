{
  "name": "@jsoma/n8n-nodes-opik",
  "version": "0.1.14",
  "description": "n8n community node for Comet Opik tracing, prompts, guardrails, and feedback.",
  "license": "MIT",
  "homepage": "https://github.com/jsoma/n8n-nodes-opik#readme",
  "keywords": [
    "n8n-community-node-package"
  ],
  "author": {
    "name": "Jonathan Soma",
    "email": "jonathan.soma@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsoma/n8n-nodes-opik.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/OpikApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Opik/Opik.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
